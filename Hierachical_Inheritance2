public class Att extends Animal{
    public void bleat() {
        System.out.println("Bleat!");
    }
     public void bark() {
        System.out.println("Bark!");

    }
    
    public static void main(String[] args) {
        // TODO code application logic here
        Att goat = new Att();
        goat.bleat();
        goat.bark();
        
        Att dog = new Att();
        dog.bleat();
        dog.bark();
      
         
    }  
        
    }
